<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Bookmark Manager</title>
  </head>

  <body>
      <% if current_user %>
      Welcome <%= current_user.email %>
      <% end %>

      <% if flash[:notice] %>
      <div class="" id="notice">
        <%= flash[:notice] %>
      </div>
      <% end %>
      
    <%= yield %>
  </body>
</html>
