<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Links</title>
  </head>
  <body>
    <div class="">
      <ul>
        <% @links.each do |link| %>
        <li>
          Title: <%= link.title %>
          URL: <%= link.url %>
        </li>
        <% end %>
      </ul>

      <% @tags.each do |tag| %>
      <li>
        Tag: <%= tag.tag %>
      </li>
      <% end %>

    </div>
  </body>
</html>
